{
  "name": "ivankristianto/wp-domain-manager",
  "description": "Domain Manager WordPress Plugin",
  "version": "0.1.0",
  "type": "wordpress-plugin",
  "keywords": [],
  "homepage": "https://www.ivankristianto.com/wp-domain-manager",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Ivan Kristianto",
      "email": "ivan@ivankristianto.com",
      "homepage": "https://www.ivankristianto.com",
      "role": "Developer"
    }
  ],
  "prefer-stable" : true,
  "require": {
    "php": ">=7.2",
    "ivankristianto/phpwhois": "~4.3.0"
  },
  "require-dev": {
    "wp-coding-standards/wpcs": "^1.0.0",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.6.2"
  },
  "autoload": {
    "psr-4": {
      "WPDM\\": "includes/classes"
    },
    "files": [
      "includes/functions/helpers.php"
    ]
  },
  "extra": {
    "installer-paths": {
      "vendor/{$name}/": [
        "type:wordpress-plugin"
      ]
    }
  }
}
